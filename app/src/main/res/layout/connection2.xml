<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    android:id="@+id/homeLayout"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fone_for_con"
    tools:context=".fragment.HomeFragment">







    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/rectangle_menu"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.13"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintWidth_percent="1">

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.18"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintWidth_percent="1">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout7"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintEnd_toStartOf="@+id/constraintLayout4"
            app:layout_constraintHeight_percent="1"
            app:layout_constraintWidth_percent="0.3">

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:src="@drawable/home_image_white"
                app:layout_constraintBottom_toTopOf="@+id/guideline_for_text_main"
                app:layout_constraintDimensionRatio="1:1"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHeight_percent="0.3"
                app:layout_constraintStart_toStartOf="parent" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline_for_light"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.55" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:textColor="#FFFFFF"
                android:gravity="center"
                android:fontFamily="@font/kyivtypeserif_bold"
                android:text="Головна"
                app:layout_constraintHeight_percent="0.15"
                app:layout_constraintTop_toTopOf="@+id/guideline_for_text_main"
                app:layout_constraintWidth_percent="1">

            </TextView>

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline_for_text_main"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.6" />

            <Button
                android:id="@+id/button_home"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#00FFFFFF">
            </Button>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout4"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHeight_percent="1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintWidth_percent="0.3">

            <ImageView
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:src="@drawable/light_image_white"
                app:layout_constraintBottom_toTopOf="@+id/guideline_for_light"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHeight_percent="0.2"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintDimensionRatio="1:1"/>

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline_for_light"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.55" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:textColor="#FFFFFF"
                android:gravity="center"
                android:fontFamily="@font/kyivtypeserif_bold"
                android:text="Освітлення"
                app:layout_constraintTop_toTopOf="@+id/guideline_for_text_light"
                app:layout_constraintHeight_percent="0.15"
                app:layout_constraintWidth_percent="1">
            </TextView>

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline_for_text_light"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.6" />

            <Button
                android:id="@+id/button_light"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#00FFFFFF">
            </Button>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/button2"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintHeight_percent="1"
            app:layout_constraintStart_toEndOf="@+id/constraintLayout4"
            app:layout_constraintWidth_percent="0.3">

            <ImageView
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:src="@drawable/bluetooth_image_white"
                app:layout_constraintBottom_toTopOf="@+id/guideline_for_bluetooth"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHeight_percent="0.23"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintDimensionRatio="1:1"/>


            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline_for_bluetooth"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.57" />



            <TextView
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:textColor="#FFFFFF"
                android:gravity="center"
                android:fontFamily="@font/kyivtypeserif_bold"
                android:text="Підключення"
                app:layout_constraintTop_toTopOf="@+id/guideline_for_text_connect"
                app:layout_constraintHeight_percent="0.15"
                app:layout_constraintWidth_percent="1">
            </TextView>


            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline_for_text_connect"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.6" />

            <Button
                android:id="@+id/button_wifi"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#00FFFFFF">
            </Button>

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:src="@drawable/on_active_element_button"
                app:layout_constraintHeight_percent="0.55"
                app:layout_constraintWidth_percent="1"
                tools:layout_editor_absoluteY="2dp" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>


